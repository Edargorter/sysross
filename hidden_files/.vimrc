syntax on
set title
set number
filetype plugin indent on
set tabstop=4
set softtabstop=4
set shiftwidth=4
set noexpandtab
set background=dark
set t_Co=256
highlight Comment ctermfg=green
set hlsearch
set ruler
set ai
set paste
set autoindent
set linebreak
nmap j gj
nmap k gk
vmap j gj
vmap k gk
nmap $ g$
vmap $ g$
nmap 0 g0
vmap 0 g0
nmap V 0v$
